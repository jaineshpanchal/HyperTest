#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/cli/dist/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/cli/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/node_modules:/Users/jaineshpanchal/Documents/GitHub/node_modules:/Users/jaineshpanchal/Documents/node_modules:/Users/jaineshpanchal/node_modules:/Users/node_modules:/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/cli/dist/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/cli/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/packages/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/node_modules:/Users/jaineshpanchal/Documents/GitHub/node_modules:/Users/jaineshpanchal/Documents/node_modules:/Users/jaineshpanchal/node_modules:/Users/node_modules:/node_modules:/Users/jaineshpanchal/Documents/GitHub/HyperTest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@hypertest/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../@hypertest/cli/dist/index.js" "$@"
fi
